{
  "env": {
    "jest": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier-standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-useless-constructor": "off"
  }
}
